{
    "lang": "pt-BR",
    "start_url": "index.html",
    "name": "Conversor de PDFs",
    "short_name": "Converter PDF",
    "icons": [
        {
            "src": "android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
    "theme_color": "#ffffff",
    "background_color": "#ffffff",
    "display": "standalone",
    "share_target": {
        "action": "/_share-target",
        "enctype": "multipart/form-data",
        "method": "POST",
        "params": {
          "files": [{
            "name": "media",
            "accept": [
              "audio/*",
              "image/*",
              "video/*",
              "application/*"
            ]
          }]
    }
  }}